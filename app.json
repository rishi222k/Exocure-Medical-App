{
  "expo": {
    "name": "MedFS",
    "slug": "MedFS",
    "version": "1.0.0",
    "icon": "./assets/AppIcon.png",
    "splash": {
      "image": "./assets/Splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "expo.ts",
        "expo.tsx",
        "expo.js",
        "expo.jsx",
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "wasm",
        "svg"
      ]
    },
    "assetBundlePatterns": [
      "**/*"
    ]
  }
}
